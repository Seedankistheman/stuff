		debounce = false
		hum.WalkSpeed = 0
		if data.Transformation.Value == "Base" or data.Transformation.Value == "SSJG" or data.Transformation.Value == "SSJB" then
		    data.Transformation.Value = "SSJ"
		    auraHandle(char,gokuAssets.SSJAura)
			hairHandle(char,gokuAssets.SSJHair)
			bodyLightning(char)
		
		elseif data.Transformation.Value == "SSJ" then
		     data.Transformation.Value = "SSJ2"
			if char.HumanoidRootPart:FindFirstChild("FormAura") then
				auraHandle(char)
			end
			auraHandle(char,gokuAssets.SSJAura)
			hairHandle(char,gokuAssets.SSJ2Hair)
		elseif data.Transformation.Value == "SSJ2" then
		    		     data.Transformation.Value = "SSJ3"
		    		   
	if char.HumanoidRootPart:FindFirstChild("FormAura") then
				auraHandle(char)
			end
			auraHandle(char,gokuAssets.SSJAura)
			hairHandle(char,gokuAssets.SSJ3Hair)
		end
		hum.WalkSpeed = 0
		debounce = true
	end
end

