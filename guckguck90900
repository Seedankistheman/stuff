    local localPlr = game.Players.LocalPlayer
local function CreateTween(obj, tb, eas, play)
	local thing13 = game:GetService("TweenService"):Create(obj, TweenInfo.new(unpack(tb)), eas)
	if play then
		thing13:Play()
	end
	return thing13
end
local function createButtonEffect(p1)
	p1.Rotation = -10
	CreateTween(p1, { 1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true}, {
		Rotation = 10
	}, true)
end
local function createChangedValue(p2)
	p2:WaitForChild("Start").Changed:Connect(function(p3)
		if p3 then
			local img = p2.ImageLabel
			p2.TextLabel.TextColor3 = Color3.new(1, 0, 0)
			img.ImageColor3 = Color3.new(1, 0, 0)
			img.Size = UDim2.new(1, 0, 0, 0)
			img.Position = UDim2.new(0, 0, 1, 0)
		end
	end)
	local time = p2.Time
	p2:WaitForChild("Activated").Changed:Connect(function(p4)
		if p4 then
			local textl = p2.TextLabel
			local imgl = p2.ImageLabel
			textl.TextColor3 = Color3.new(1, 0, 0)
			imgl.ImageColor3 = Color3.new(1, 0, 0)
			imgl.Size = UDim2.new(1, 0, 0, 0)
			imgl.Position = UDim2.new(0, 0, 1, 0)
			CreateTween(imgl, { time.Value, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, 0, false, 0 }, {
				Position = UDim2.new(0, 0, 0, 0), 
				Size = UDim2.new(1, 0, 1, 0)
			}, true)
			task.wait(time.Value)
			imgl.ImageColor3 = Color3.new(1, 1, 1)
			textl.TextColor3 = Color3.new(1, 1, 1)
		end
	end)
	
end
	cooldownsTable = {}
	moveDebounce = game.Players.LocalPlayer.PlayerGui.UI.Ui.MoveDebounceShower
local function cooldowns(name, val)
	if cooldownsTable[name] == nil then
		cooldownsTable[name] = true
		return cooldownsTable[name]
	end
	if val ~= nil then
		cooldownsTable[name] = val
		return
	end
	return cooldownsTable[name]
end

spawn(function() while task.wait() do if char.Effects:FindFirstChild("EnergyPart") then char.Attacks.YellowBlast.Position =char.Effects:FindFirstChild("EnergyPart").Position
elseif not char.Effects:FindFirstChild("EnergyPart") then game.Players.LocalPlayer.Character.Attacks.YellowBlast.Position = Vector3.new(50000,50000,50000)
    end end end)

local function uiCooldowns(id,start,cooldown,combatid)
	if start then
		moveDebounce["Moves" .. combatid][id].Start.Value = true
		moveDebounce["Moves" .. combatid][id].Start.Value = false
		return
	end
	moveDebounce["Moves" .. combatid][id].Time.Value = cooldown
	moveDebounce["Moves" .. combatid][id].Activated.Value = true
	moveDebounce["Moves" .. combatid][id].Activated.Value = false
end
	debounce = true 
	inputbegan[Enum.KeyCode.One] = function()
	if debounce and not checkIfHurt() then
		if currentType == "DeltaSans" and data.Stamina.Value >= 25 and cooldowns("Delta1",nil) then
			debounce = false
			_G.RootPartFollow = false
			cooldowns("Delta1", false)
			uiCooldowns(1, true, nil, 1)
			HumanoidRootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
			 local args = {
    [1] = {
        [1] = getrenv()._G.Pass,
        [2] = "Rhabdophobia",
        [3] = True,
        [4] = game:GetService("Players").LocalPlayer.UserId
    }
}

game:GetService("ReplicatedStorage").Remotes.BettyMoves:InvokeServer(unpack(args))
char:WaitForChild("Rhad")
	--	game.ReplicatedStorage.Remotes.DeltaSansMoves:InvokeServer({ getrenv()._G.Pass, "Combat1", HumanoidRootPart.CFrame, HumanoidRootPart  })
		local args = {
    [1] = {
        [1] = getrenv()._G.Pass,
        [2] = "Combat1",
        [4] = HumanoidRootPart.CFrame,
        [5] = HumanoidRootPart
    }
}

game:GetService("ReplicatedStorage").Remotes.DeltaSansMoves:InvokeServer(unpack(args))
			uiCooldowns(1, false, 30, 1)
			_G.RootPartFollow = true
			debounce = true
			task.wait(30)
			cooldowns("Delta1", true)
		elseif currentType == "Goku" and data.Stamina.Value >= 50 and cooldowns("My1",nil) then
			debounce = false
			cooldowns("My1", false)
			uiCooldowns(1, true, nil, 2)
		local anim = Instance.new("Animation")
				anim.AnimationId = "rbxassetid://3320797123"
				chargeanim[char] = char.Humanoid:LoadAnimation(anim)
				chargeanim[char].KeyframeReached:Connect(function(k)
					if k == "Pause" and chargeanim[char] then
						chargeanim[char]:AdjustSpeed(0)
					end
				end)
				chargeanim[char]:Play(0.15)
				local charge = gokuAssets.Kamehameha.EnergyPart:Clone()
				local weld = Instance.new("Weld")
				weld.Part0 = char["Left Arm"]
				weld.Part1 = charge
				weld.C0 = CFrame.new(0.5,-1.4,0)
				weld.Parent = charge
				charge.Parent = char.Effects
				charge.Sound:Play()
				charge.Sound.Ended:Connect(function()
					if charge.Parent then
						charge.Sound.TimePosition = 31
					--	charge:Pause()
					end
				end)
				for i,v in pairs(charge:GetChildren()) do
					if v:IsA("Attachment") and v:FindFirstChild("OriginalPosition") then
						tweenS:Create(v,TweenInfo.new(25),{Position = v.OriginalPosition.Value}):Play()
					end
				end
				tweenS:Create(charge:WaitForChild("KMLight"),TweenInfo.new(25),{Brightness = 12}):Play()
				task.spawn(function()
					while charge and charge.Parent do
	
						weld.C0 *= CFrame.Angles(math.rad(1),math.rad(1),0)
						task.wait(0.01)
					end
				end)
			while true do
				if not uis:IsKeyDown(Enum.KeyCode.One) then
					break
				end
				if data.Stamina.Value <= 0 then
					break
				end
				task.wait(0.1)
			end
			_G.RootPartFollow = false;
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 0
			local charge = char.Effects:FindFirstChild("EnergyPart")
				local pos = charge.Sound.TimePosition
				local level = math.clamp(math.round(pos/3),1,10)
				charge:Destroy()
				spawn(function() for i = 1,15 do 
					game.Players.LocalPlayer.Character.Attacks.YellowBlast.Position = Vector3.new(50000,50000,50000) end end)
				createBeam(plr,char,getmouser(),"Kamehameha",level)
			uiCooldowns(1, false, 10, 2)
			_G.RootPartFollow = true
			debounce = true
			task.wait(10)
			cooldowns("My1", true)
		end
	end
end
	
	
local function wkam()
    enemy = game:GetService("Players").LordsNutSock.Backpack.Main.LockOnScript.LockOn.Value
    		local iframe = Instance.new("ForceField")
			iframe.Name = "Inv"
			iframe.Visible = false
			iframe.Parent = char
			local current = char.HumanoidRootPart.CFrame
			local anim = Instance.new("Animation")
			anim.AnimationId = "rbxassetid://4820780935"
			anim = char.Humanoid:LoadAnimation(anim)
			local hit = Instance.new("StringValue")
			hit.Name = "Hit"
			hit.Parent = enemy
		--	playSound(char,gokuSounds.Teleport)
			local two = gokuAssets.ZanzokenParticles:Clone()
			local weld = Instance.new("Weld")
			weld.Part0 = char.Torso
			weld.Part1 = two
			weld.Parent = two
			two.Parent = char.Effects
			char:SetPrimaryPartCFrame(enemy.HumanoidRootPart.CFrame)
			task.spawn(function()
				task.wait(0.2)
				two.ParticleEmitter1.Enabled = false
				two.ParticleEmitter.Enabled = false
				game.Debris:AddItem(two,0.15)
			end)
			local clash = char.Humanoid:LoadAnimation(rep.Animations.Clashing)
			clash:Play()
			for i=1,30 do
				if enemy and enemy.Parent then
					if enemy.Data.Blocking.Value then
						enemy.Data.Blocking.Value = false
						enemy.Data.PerfectBlocking.Value = false
					end
				char = game.Players.LocalPlayer.Character
local args = {
    [1] = getrenv()._G.Pass,
    [2] = enemy,
    [3] = {
        ["Type"] = "Normal",
        ["HitEffect"] = "HeavyHitEffect",
        ["HurtAnimation"] = game:GetService("ReplicatedStorage").Animations.HurtAnimations["Hurt" .. math.random(1, 3)],
        ["Velocity"] = Vector3.new(0,1,0),
        ["Position"] = char.HumanoidRootPart.Position + char.HumanoidRootPart.CFrame.LookVector * 3,
        ["CameraShake"] = "Bump",
        ["HitTime"] = 2,
        ["VictimCFrame"] =  CFrame.new(enemy.HumanoidRootPart.Position, char.HumanoidRootPart.Position) ,
        ["Sound"] = game:GetService("ReplicatedStorage").Sounds.Punch,
        ["CombatInv"] = true,
        ["CancelKnockback"] = true,
        ["Damage"] = 0.5
