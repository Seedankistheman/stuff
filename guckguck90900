local p = game.Players.LocalPlayer


local tweenS = game:GetService("TweenService")

local mathrandom = math.random
local cf = CFrame.new
local cfangles = CFrame.Angles
local vector = Vector3.new
local mathrad = math.rad

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Effects").OnClientEvent:Connect(function(args)
	if args[1] == "Kamehameha" then
		local model = args[2]
		local sizing = args[3]
		local sizey = args[4]
		local sizer = {}
		local front = {}
		local back = {}
		local center = model:WaitForChild("Center")
		for i,v in pairs(model:GetChildren()) do
			if v.Name:match("Sizing") then
				sizer[v] = {X=v.Size.X,Y=v.Size.Y,Z=v.Size.Z,CF=v.CFrame}
				v.Size = vector(v.Size.X,0,v.Size.Z)
			elseif v.Name:match("Front") then
				local w = v:FindFirstChild("Weld")
				if w then
					front[w] = w.C0
				end
			elseif v.Name:match("Back") then
				table.insert(back,v)
			end
		end
		for i,v in pairs(back) do
			local original = v.Size
			tweenS:Create(v,TweenInfo.new(0.15,Enum.EasingStyle.Sine,Enum.EasingDirection.Out),{Size = original + Vector3.new(original.X * 2.3,0,original.Z * 2.3)}):Play()
			task.spawn(function()
				task.wait(0.15)
				if back then
					tweenS:Create(v,TweenInfo.new(0.15,Enum.EasingStyle.Sine,Enum.EasingDirection.In),{Size = original}):Play()
				end
			end)
		end
		local deb = false
		while model and model.Parent and sizing and sizing.Parent and sizing.Value do
			local random = mathrandom(-10, 10) / 100
			for part,info in pairs(sizer) do
				part.Size = vector(info.X + random, sizey.Value, info.Z + random)
				part.CFrame = info.CF + center.CFrame.LookVector * (sizey.Value / 2 - info.Y/2)
			end
			for i,v in pairs(back) do
				v.CFrame = v.CFrame * cfangles(0,mathrad(mathrandom(10,60)),0)
			end
			for w,cfr in pairs(front) do
				w.C0 = cfr * cf(0,-(sizey.Value/2 - sizer[w.Part0].Y/2),0) * cfangles(0,mathrad(-(mathrandom(10,60))),0)
			end
			if not deb then
				deb = true
				task.spawn(function()
					local clone = model:FindFirstChild("BeamAura"):Clone()
					clone.Name = "Cloned"
					for i,v in pairs(clone:GetChildren()) do
						v.Transparency = 0
					end
					clone.Parent = model
					tweenS:Create(clone,TweenInfo.new(0.35),{CFrame = clone.CFrame * cf(0,sizey.Value,0)}):Play()
					task.wait(0.25)
					for i,v in pairs(clone:GetChildren()) do
						tweenS:Create(v,TweenInfo.new(0.1),{Transparency=1}):Play()
					end
					game.Debris:AddItem(clone,0.1)
					task.wait(0.2)
					deb = false
				end)
			end
			task.wait()
		end
		sizer = nil
		back = nil
		front = nil
		if model then
			for i,v in pairs(model:GetChildren()) do
				if v:IsA("MeshPart") then
					tweenS:Create(v,TweenInfo.new(0.25,Enum.EasingStyle.Sine,Enum.EasingDirection.InOut),{Size = Vector3.new(0,v.Size.Y,0)}):Play()
				end
			end
			game.Debris:AddItem(model,0.25)
		end
	end
end)

local function getlock()
		return game.Players.LocalPlayer.Backpack.Main.LockOnScript.LockOn.Value
	end


