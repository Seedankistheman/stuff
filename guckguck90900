su={
[1] = "".." ",
[2] = "".." ",
[3] = "Has Been Made!",
}
function fold(t)
su[1] = "File".." "
su[2] = t.." "
if not isfile(t) then
    makefolder(t)
    print(su[1]..su[2]..su[3])
end
return t
end
function wf(sn,au)
if not isfile(sn) then
    local an=au
    if string.find(au, "https://cdn.discordapp.com/attachments/") then
        re = syn.request({Url = au, Method = "GET"}).Body
    else error("No")
    end
    writefile(sn,re)
end
return getsynasset(sn)
end
wf("Ultra_Instinct_Theme_Remix.mp3", "https://cdn.discordapp.com/attachments/938155251146580032/1054951602445885470/Ultra_Instinct_Theme_Remix.mp3")

