local char = game.Players.LocalPlayer.Character
local cf = char.Torso.CFrame
	local trans = {}
	local function handleAccessories(t)
		for i,ac in pairs(char:GetChildren()) do
			if ac:IsA("Accessory") then
				local handle = ac:FindFirstChild("Handle")
				if handle and (handle:FindFirstChild("HatAttachment") or handle:FindFirstChild("HairAttachment")) then
					if t then
						trans[ac] = handle.Transparency
						handle.Transparency = 1
					elseif trans[ac] then
						handle.Transparency = trans[ac]
						trans[ac] = nil
					end
				end
			end
		end
	end
	local function hairHandle(hair)
		local old = char:FindFirstChild("FormHair")
		if old then
			pcall(function()
				old.Name = "Removing"
				tweenS:Create(old.Hair,TweenInfo.new(0.5),{Transparency=1}):Play()
				if old.Hair:FindFirstChild("Hair") then
					tweenS:Create(old.Hair.Hair,TweenInfo.new(0.5),{Transparency=1}):Play()
				end
				game.Debris:AddItem(old,0.5)
			end)
			handleAccessories(char)
		end
		if hair then
			handleAccessories(char,true)
			local newhair = hair:Clone()
			newhair.Name = "FormHair"
			local weld = Instance.new("WeldConstraint")
			weld.Part0 = char.Head
			newhair:SetPrimaryPartCFrame(char.Head.CFrame)
			weld.Part1 = newhair.Hair
			weld.Parent = newhair
			newhair.Parent = char
			return newhair
		end
	end
		local function auraHandle(aura,entireBody, name)
		if not aura then
			for i,v in pairs(char:GetDescendants()) do
				if (v.Name == (name or "FormAura") or v.Parent.Name == (name or "FormAura")) and v:IsA("ParticleEmitter") then
					v.Name = "Removing"
					v.Enabled = false
				elseif v.Name == (name or "FormAura") and not v:IsA("BoolValue") then
					game.Debris:AddItem(v,1.5)
				end
			end
		else
			for i,v in pairs(aura:GetChildren()) do
				if not entireBody then
					local clone = v:Clone()
					clone.Name = (name or "FormAura")
					clone.Parent = char.HumanoidRootPart
					if v:IsA("Sound") then
						v:Play()
					end
				else
					for i,part in pairs(char:GetChildren()) do
						if (part:IsA("BasePart") or part:IsA("MeshPart") or part:IsA("Part")) and part.Transparency ~= 1 then
							if ((v:IsA("PointLight") or v:IsA("Sound")) and part.Name == "Torso") then
								local clone = v:Clone()
								clone.Name = (name or "FormAura")
								clone.Parent = part
								if v:IsA("Sound") then
									v:Play()
								end
							elseif v:IsA("ParticleEmitter") then
								local clone = v:Clone()
								clone.Name = (name or "FormAura")
								clone.Parent = part
							end
						end
					end
				end
			end
		end
		end





