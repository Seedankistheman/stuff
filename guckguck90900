local function celShading(color,transp,secondaryColor)
		if color then
			for i,p in pairs(char:GetChildren()) do
				if p.Name == "Left Arm" or p.Name == "Right Arm" or p.Name == "Torso" or p.Name == "Head" or p.Name == "Left Leg" or p.Name == "Right Leg" then
					local cel = gokuAssets.CelShade:FindFirstChild(p.Name):Clone()
					cel.Color = color
					cel.Transparency = transp
					cel.Name = "CelShade"
					local weld = Instance.new("WeldConstraint")
					weld.Part0 = p
					cel.CFrame = p.CFrame
					weld.Part1 = cel
					weld.Parent = cel
					cel.Parent = p
					if secondaryColor then
						tweenS:Create(cel,secondary,{Color = secondaryColor}):Play()
					end
				end
			end
		else
			for i,p in pairs(char:GetChildren()) do
				if p.Name == "Left Arm" or p.Name == "Right Arm" or p.Name == "Torso" or p.Name == "Head" or p.Name == "Left Leg" or p.Name == "Right Leg" then
					local cel = p:FindFirstChild("CelShade")
					if cel then cel:Destroy() end
				end
			end
		end
	end
			lightningTable = {}
				local ti = TweenInfo.new(0.09,Enum.EasingStyle.Sine,Enum.EasingDirection.InOut)
				local function Lightning(Part0, Part1, Times, Offset, Color, Thickness, Trans, SizeAdd, part0ok)
		local magz = (Part0 - Part1).magnitude
		local curpos = Part0
		local trz = {
			-Offset,
			Offset
		}
		for i = 1, Times do
			local li = Instance.new("Part")
			li.Name = "Lightning"
			li.TopSurface = 0
			li.Material = "Neon"
			li.BottomSurface = 0
			li.Locked = true
			li.CastShadow = false
			li.Transparency = 0
			li.BrickColor = Color
			li.formFactor = "Custom"
			li.CanCollide = false
			li.CanTouch = false
			li.CanQuery = false
			li.Massless = true
			local weld = Instance.new("WeldConstraint")
			weld.Parent = li
			weld.Part0 = part0ok
			weld.Part1 = li
			game.Debris:AddItem(li,4)
			li.Size = Vector3.new(Thickness, Thickness, magz / Times)
			local Offzet = Vector3.new(trz[math.random(1, 2)], trz[math.random(1, 2)], trz[math.random(1, 2)])
			local trolpos = CFrame.new(curpos, Part1) * CFrame.new(0, 0, magz / Times).Position + Offzet
			if Times == i then
				local magz2 = (curpos - Part1).magnitude
				li.Size = Vector3.new(Thickness, Thickness, magz2)
				li.CFrame = CFrame.new(curpos, Part1) * CFrame.new(0, 0, -magz2 / 2)
			else
				li.CFrame = CFrame.new(curpos, trolpos) * CFrame.new(0, 0, magz / Times / 2)
			end
		
			li.Parent = workspace.Parts
			curpos = li.CFrame * CFrame.new(0, 0, magz / Times / 2).Position
			task.spawn(function()
				task.wait(math.random(500,800)/10000)
				tweenS:Create(li,ti,{Transparency=1}):Play()
				game.Debris:AddItem(li, 0.12)
			end)
		end
	end
	local function bodyLightning(color ,rate)
		if not color then -- if no color given it will deactivate
			lightningTable[char] = nil
		else
			task.spawn(function()
				local bodyParts = {}
				for i,p in pairs(char:GetChildren()) do
					if (p:IsA("Part") or p:IsA("MeshPart")) and p.Transparency ~= 1 then
						table.insert(bodyParts,p)
					end
				end
				local bounding = {}
				for _, v in pairs(bodyParts) do
					local temp = {
						X = nil,
						Y = nil,
						Z = nil
					}
					temp.X = v.Size.X / 2 * 35
					temp.Y = v.Size.Y / 2 * 35
					temp.Z = v.Size.Z / 2 * 35
					bounding[v.Name] = temp
				end
				local cd = tick()
				lightningTable[char] = cd
				local deb = false
				while char and lightningTable[char] == cd  do
					task.wait(math.random(130,600)/1000)
				
					for i=1,rate do
						local Body1 = bodyParts[math.random(#bodyParts)]
						local Body2 = bodyParts[math.random(#bodyParts)]
						local Pos1 = Vector3.new(math.random(-bounding[Body1.Name].X, bounding[Body1.Name].X) / 55, math.random(-bounding[Body1.Name].Y, bounding[Body1.Name].Y) / 55, math.random(-bounding[Body1.Name].Z, bounding[Body1.Name].Z) / 55)
						local Pos2 = Vector3.new(math.random(-bounding[Body2.Name].X, bounding[Body2.Name].X) / 55, math.random(-bounding[Body2.Name].Y, bounding[Body2.Name].Y) / 55, math.random(-bounding[Body2.Name].Z, bounding[Body2.Name].Z) / 55)
						local SPos1 = Body1.Position + Pos1
						local SPos2 = Body2.Position + Pos2
						Lightning(SPos1, SPos2, 4, 3, BrickColor.new(color), 0.05, 0.05, -0.1, Body1)
					end
				end
			end)
		end
	end
	debounce = true
