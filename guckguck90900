Child:WaitForChild("Trail").Color = ColorSequence.new(Color3.fromRGB(0, 255, 255))
Child:WaitForChild("Particle").Burst.Color = ColorSequence.new(Color3.fromRGB(0, 255, 255))
local start = Child.CFrame
local ending = Vector3.new(math.random(-80,80)/10,math.random(-80,80)/10,math.random(-80,80)/10)
local middle = Child.Position +Child.CFrame.LookVector * 40
Child.Anchored = true 
wait(0.2)
Child.Anchored = false
enemy = game:GetService("Players").LocalPlayer.Backpack.Main.LockOnScript.LockOn.Value
