local blasts = false
   local function lerp(p0, p1, t)
        return p0*(1-t) + p1*t
    end
    local function quadBezier(p0, p1, p2, t)
        local l1 = lerp(p0, p1, t)
        local l2 = lerp(p1, p2, t)
        local quad = lerp(l1, l2, t)
        return quad
    end


game.Players.LocalPlayer.Character.Attacks.ChildAdded:Connect(function(Child)
if Child.Name:match("YellowBlast") and blasts == true then
Child.Name = "JusticeCaught"
Child:WaitForChild("BodyVelocity").Velocity = Vector3.new(0,0,0)
Child:WaitForChild("Particle").ParticleEmitter.Color = ColorSequence.new(Color3.fromRGB(0, 255, 255))
