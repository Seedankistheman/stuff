local function getAllCooldowns()
	local ha = {}
	for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.UI.Ui.MoveDebounceShower:GetChildren()) do
		if string.match(v.Name, "Moves") then
			for a,b in pairs(v:GetChildren()) do
				if b.Name ~= "Indicator" then
					ha[b] = {
						Moveset = v.Name, 
						Activated = b:WaitForChild("Activated"), 
						Time = b:WaitForChild("Time")
					}
					createButtonEffect(b)
					createChangedValue(b)
				end
			end
		end
	end
	return ha
end
task.wait(0.5)
getAllCooldowns()

    
    end)




