		local function checkIfHurt()
		return char:FindFirstChild("Hit") or char:FindFirstChild("Debounce") or char.Humanoid.Health <= 0
	end
	

	
	
	local function createBeam(plr,char,direction,beamName,level,noadjust)
		local beam = gokuAssets[beamName]["Beam"..level]:Clone()
		local part = beam:WaitForChild("Sizing3")
		local center = beam:WaitForChild("Center")
		local beamcf,beamsize = beam:GetBoundingBox()
		local active = Instance.new("BoolValue")
		active.Value = true
		active.Parent = beam
		local val = Instance.new("NumberValue")
			spawn(function() while beam do 
			    needed = true 
			    TrailPart.Position = beam.Front1.Position task.wait() TrailPart.Position = beam.Back1.Position task.wait()	
		    TrailPart.Anchored = false
	 end
	 			    needed = false
	 
	 end)
		val.Value = 4
		val.Parent = beam
		if not noadjust then
			char:SetPrimaryPartCFrame(CFrame.new(char.HumanoidRootPart.Position,Vector3.new(direction.X,char.HumanoidRootPart.Position.Y,direction.Z)))
		end	
		beam:SetPrimaryPartCFrame(CFrame.new(char.HumanoidRootPart.Position,direction))
		beam.Parent = char.Effects
		spawn(function()
		    		local model = beam
		local sizing = active
		local sizey = val
		local sizer = {}
		local front = {}
		local back = {}
		local center = model:WaitForChild("Center")
		for i,v in pairs(model:GetChildren()) do
			if v.Name:match("Sizing") then
				sizer[v] = {X=v.Size.X,Y=v.Size.Y,Z=v.Size.Z,CF=v.CFrame}
				v.Size = vector(v.Size.X,0,v.Size.Z)
			elseif v.Name:match("Front") then
				local w = v:FindFirstChild("Weld")
				if w then
					front[w] = w.C0
				end
			elseif v.Name:match("Back") then
				table.insert(back,v)
			end
		end
		for i,v in pairs(back) do
			local original = v.Size
			tweenS:Create(v,TweenInfo.new(0.15,Enum.EasingStyle.Sine,Enum.EasingDirection.Out),{Size = original + Vector3.new(original.X * 2.3,0,original.Z * 2.3)}):Play()
			task.spawn(function()
				task.wait(0.15)
				if back then
					tweenS:Create(v,TweenInfo.new(0.15,Enum.EasingStyle.Sine,Enum.EasingDirection.In),{Size = original}):Play()
				end
			end)
		end
		local deb = false
		while model and model.Parent and sizing and sizing.Parent and sizing.Value do
			local random = mathrandom(-10, 10) / 100
			for part,info in pairs(sizer) do
				part.Size = vector(info.X + random, sizey.Value, info.Z + random)
				part.CFrame = info.CF + center.CFrame.LookVector * (sizey.Value / 2 - info.Y/2)
			end
			for i,v in pairs(back) do
				v.CFrame = v.CFrame * cfangles(0,mathrad(mathrandom(10,60)),0)
			end
			for w,cfr in pairs(front) do
				w.C0 = cfr * cf(0,-(sizey.Value/2 - sizer[w.Part0].Y/2),0) * cfangles(0,mathrad(-(mathrandom(10,60))),0)
			end
			if not deb then
				deb = true
				task.spawn(function()
					local clone = model:FindFirstChild("BeamAura"):Clone()
					clone.Name = "Cloned"
					for i,v in pairs(clone:GetChildren()) do
						v.Transparency = 0
					end
					clone.Parent = model
					tweenS:Create(clone,TweenInfo.new(0.35),{CFrame = clone.CFrame * cf(0,sizey.Value,0)}):Play()
					task.wait(0.25)
					for i,v in pairs(clone:GetChildren()) do
						tweenS:Create(v,TweenInfo.new(0.1),{Transparency=1}):Play()
					end
					game.Debris:AddItem(clone,0.1)
					task.wait(0.2)
					deb = false
				end)
			end
			task.wait()
		end
	
		sizer = nil
		back = nil
		front = nil
		if model then
			for i,v in pairs(model:GetChildren()) do
				if v:IsA("MeshPart") then
					tweenS:Create(v,TweenInfo.new(0.25,Enum.EasingStyle.Sine,Enum.EasingDirection.InOut),{Size = Vector3.new(0,v.Size.Y,0)}):Play()
				end
			end
			game.Debris:AddItem(model,0.25)
		end

		    end)


		local tween = tweenS:Create(val,TweenInfo.new(level + 1),{Value = 250 + (level*50)})
		tween:Play()
		local wallhit = false
		local hitdeb = false
		local cancelled = false


	
		task.spawn(function()
			task.wait(level + 1)
			if not cancelled and beam and beam.Parent and char and char.Parent then
				if tween then
					tween:Cancel()
					tween = nil
				end
				active.Value = false
				if chargeanim[char] then
					chargeanim[char]:Stop(0.15)
					chargeanim[char] = nil
				end
		local beamcf,beamsize = beam:GetBoundingBox()
				for i,v in pairs(gettouched(part.CFrame + center.CFrame.LookVector * (val.Value / 2 - part.Size.Y/2), Vector3.new(part.Size.X,val.Value + (beamsize.Z - part.Size.Y),part.Size.Z), nil))
do 		 spawn(function()
         for _,xv in pairs(mytab) do xv.Base:WaitForChild("RemoteEvent"):FireServer(v.Parent) end
        end) end 
				game.Debris:AddItem(beam,1)

			end
		end)
		while tween and beam and beam.Parent and char and char.Parent and not cancelled do
			if not hitdeb then
				hitdeb = true
								

				task.spawn(function()
					task.wait(0.1)
					hitdeb = false
				end)
						local beamcf,beamsize = beam:GetBoundingBox()
			for i,v in pairs(gettouched(part.CFrame + center.CFrame.LookVector * (val.Value / 2 - part.Size.Y/2), Vector3.new(part.Size.X,val.Value + (beamsize.Z - part.Size.Y),part.Size.Z), nil))

do 
    
    			 spawn(function()
         for _,xv in pairs(mytab) do xv.Base:WaitForChild("RemoteEvent"):FireServer(v.Parent) end
        end)
    end
			end
			if checkIfHurt(char) then
				cancelled = true
				chargeanim[char]:Stop(0.15)
				chargeanim[char] = nil
				if tween then
					tween:Cancel()
					tween = nil
				end
				game.Debris:AddItem(beam,1)
				active.Value = false
				break
			end
			if not wallhit then
				local ray = workspace:Raycast(center.Position,center.CFrame.LookVector * (val.Value + part.Size.Y/2),rayParams)
							
				if ray and ray.Instance and tween and not ray.Instance:IsDescendantOf(char) then

					wallhit = true
					tween:Cancel()
					task.spawn(function()
						while active and active.Parent and active.Value do
						    						    for i,v in pairs(getconnections(game.ReplicatedStorage.Remotes.Effects.OnClientEvent)) do
v:Fire({"Rocks",ray.Instance,CFrame.new(ray.Position),5,1.2})
end
		
						--	main.shakeNearbyScreens("Explosion", char.Torso.Position, 80 + (level*10))
							task.wait(0.2)
						end
					end)
				end
			end
			task.wait()
		end

	end

	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	

