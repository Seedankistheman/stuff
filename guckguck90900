local function gettouched(CFramee, what, part)
    local v11 = OverlapParams.new()
v11.FilterType = Enum.RaycastFilterType.Whitelist
local v20 = CFramee
		local v21 = what
		local v22 = part
		if v22 then
			v20 = v22.CFrame
		end
		local v23 = {}
		for v24, v25 in pairs(workspace:GetChildren()) do
			if
				v25:FindFirstChild("HumanoidRootPart") and v25.Name ~=game.Players.LocalPlayer.Name and
				not v25:FindFirstChild("Inv")
			then
				table.insert(v23, v25.HumanoidRootPart)
			end
		end
		v11.FilterDescendantsInstances = v23
		return workspace:GetPartBoundsInBox(v20, typeof(v21) == "Vector3" and v21 or Vector3.new(v21, v21, v21), v11)
end

