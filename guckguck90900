uiLight.Position = root.Position - Vector3.new(0,3,0)
uiLight.Parent = char.Effects
for i=1,6 do
	for i,v in pairs(uiLight:GetChildren()) do
		if v:IsA("Beam") then
			v.Enabled = math.random(1,3) == 3
		end
	end
	task.wait(.11)
end
uiLight:Destroy()
task.wait(.4)
local ui2 = gokuAssets.UIEffect2:Clone()
