spawn(function()
    local tweenS = game:GetService("TweenService")
local gokuAssets = game:GetService("ReplicatedStorage").GokuStuff.GokuAssets
local gokuSounds = game:GetService("ReplicatedStorage").GokuStuff.GokuSounds
local char = game.Players.LocalPlayer.Character
