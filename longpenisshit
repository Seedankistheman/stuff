						table.insert(casted, v.Torso)
					end
				end
			end
		end
		if hitSelf then
            table.insert(casted, plr.Character.HumanoidRootPart)
		end
            if stack.Value then
            stack.Changed:Wait()
            return casted
        else
            return casted
        end
	end
