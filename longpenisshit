local folder = game:GetObjects('rbxassetid://8575468340')[1]
folder.Parent = game.Players.LocalPlayer.Character

local folder = game:GetObjects('rbxassetid://8575149448')[1]
folder.Parent = game.Players.LocalPlayer.Character

